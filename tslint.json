{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "no-trailing-whitespace": [
      true,
      "ignore-blank-lines",
      "ignore-jsdoc"
    ],
    "variable-name": [
      true,
      "ban-keywords"
    ],
    "jsdoc-format": false,
    "max-line-length": false,
    "no-bitwise": false
  },
  "rulesDirectory": []
}
