{
  "name": "mapwize-ui",
  "version": "2.4.2",
  "description": "Fully featured and ready to use Widget to add Mapwize Indoor Maps and Navigation in your Web app",
  "author": "Mapwize",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mapwize/mapwize-ui-js"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dist": "npm ci && npm run build",
    "start": "webpack-dev-server --config webpack.dev.js",
    "analyze": "webpack --config webpack.analyze.js",
    "test": "concurrently --kill-others --success=first \"http-server ./ -p 8888 > /dev/null 2>&1\" \"wdio tests/conf/local.conf.js\"",
    "test-build": "npm run build && concurrently --kill-others --success=first \"http-server ./ -p 8888 > /dev/null 2>&1\" \"wdio tests/conf/local.conf.js\"",
    "test-browserstack": "concurrently --kill-others --success=first \"http-server ./ -p 8888 > /dev/null 2>&1\" \"wdio tests/conf/browserstack.conf.js\"",
    "test-browserstack-build": "npm run build && concurrently --kill-others --success=first \"http-server ./ -p 8888 > /dev/null 2>&1\" \"wdio tests/conf/browserstack.conf.js\"",
    "integrate": "webpack --watch --config webpack.integrate.js",
    "docs": "jsdoc -c ./jsdoc.conf"
  },
  "devDependencies": {
    "@babel/cli": "7.8.3",
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@types/bootstrap": "4.3.1",
    "@types/jquery": "3.3.31",
    "@types/lodash": "4.14.149",
    "@types/node": "13.1.8",
    "@wdio/cli": "5.18.6",
    "@wdio/local-runner": "5.18.6",
    "@wdio/mocha-framework": "5.18.6",
    "@wdio/spec-reporter": "5.18.6",
    "@wdio/sync": "5.18.6",
    "autoprefixer": "9.7.4",
    "babel-loader": "8.0.6",
    "babel-preset-minify": "0.5.1",
    "browserstack-local": "1.4.4",
    "chromedriver": "79.0.0",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "5.0.2",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.2",
    "docdash": "1.1.1",
    "generate-json-webpack-plugin": "0.3.1",
    "glob": "7.1.6",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.12.1",
    "jsdoc": "3.6.3",
    "jsdoc-babel": "0.5.0",
    "license-webpack-plugin": "2.1.3",
    "node-sass": "4.13.1",
    "npx": "10.2.1",
    "postcss-loader": "3.0.0",
    "precss": "4.0.0",
    "sass-lint": "1.13.1",
    "sass-loader": "8.0.2",
    "source-map-loader": "0.2.4",
    "style-loader": "1.1.3",
    "ts-loader": "6.2.1",
    "tslint": "5.20.1",
    "typescript": "3.7.5",
    "uglifyjs-webpack-plugin": "2.2.0",
    "wdio-chromedriver-service": "5.0.2",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "bootstrap": "4.4.1",
    "core-js": "3.6.4",
    "jquery": "3.4.1",
    "lodash": "4.17.15",
    "mapwize": "4.2.1",
    "popper.js": "1.16.0",
    "regenerator-runtime": "0.13.3"
  },
  "private": true
}
