{
  "name": "mapwize-ui",
  "version": "2.5.8",
  "description": "Fully featured and ready to use Widget to add Mapwize Indoor Maps and Navigation in your Web app",
  "author": "Mapwize",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mapwize/mapwize-ui-js"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dist": "npm ci && npm run build",
    "start": "webpack-dev-server --config webpack.dev.js",
    "analyze": "webpack --config webpack.analyze.js",
    "test": "concurrently --kill-others --success=first \"http-server ./ -p 8888 > /dev/null 2>&1\" \"wdio tests/conf/local.conf.js\"",
    "test-build": "npm run build && concurrently --kill-others --success=first \"http-server ./ -p 8888 > /dev/null 2>&1\" \"wdio tests/conf/local.conf.js\"",
    "test-browserstack": "concurrently --kill-others --success=first \"http-server ./ -p 8888 > /dev/null 2>&1\" \"wdio tests/conf/browserstack.conf.js\"",
    "test-browserstack-build": "npm run build && concurrently --kill-others --success=first \"http-server ./ -p 8888 > /dev/null 2>&1\" \"wdio tests/conf/browserstack.conf.js\"",
    "integrate": "webpack --watch --config webpack.integrate.js",
    "docs": "jsdoc -c ./jsdoc.conf"
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-typescript": "7.10.1",
    "@types/bootstrap": "4.5.0",
    "@types/jquery": "3.3.38",
    "@types/lodash": "4.14.155",
    "@types/node": "14.0.13",
    "@wdio/cli": "6.1.19",
    "@wdio/local-runner": "6.1.19",
    "@wdio/mocha-framework": "6.1.19",
    "@wdio/spec-reporter": "6.1.14",
    "@wdio/sync": "6.1.14",
    "autoprefixer": "9.8.0",
    "babel-loader": "8.1.0",
    "babel-preset-minify": "0.5.1",
    "browserstack-local": "1.4.5",
    "chromedriver": "83.0.0",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "5.2.0",
    "copy-webpack-plugin": "6.0.2",
    "css-loader": "3.6.0",
    "docdash": "1.2.0",
    "generate-json-webpack-plugin": "1.0.0",
    "glob": "7.1.6",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.3.0",
    "http-server": "0.12.3",
    "jsdoc": "3.6.4",
    "jsdoc-babel": "0.5.0",
    "license-webpack-plugin": "2.2.0",
    "node-sass": "4.14.1",
    "npx": "10.2.2",
    "postcss-loader": "3.0.0",
    "precss": "4.0.0",
    "sass-lint": "1.13.1",
    "sass-loader": "8.0.2",
    "source-map-loader": "1.0.0",
    "style-loader": "1.2.1",
    "ts-loader": "7.0.5",
    "tslint": "6.1.2",
    "typescript": "3.9.5",
    "uglifyjs-webpack-plugin": "2.2.0",
    "wdio-chromedriver-service": "6.0.3",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "bootstrap": "4.4.1",
    "core-js": "3.6.5",
    "jquery": "3.5.1",
    "lodash": "4.17.19",
    "mapwize": "4.2.10",
    "popper.js": "1.16.1",
    "regenerator-runtime": "0.13.5"
  },
  "private": true
}
